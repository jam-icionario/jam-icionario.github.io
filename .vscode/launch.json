    },
    {
      "name": "run Scores Processor",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "dotnet: build",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/ScoresProcessor/bin/Debug/net9.0/ScoresProcessor.dll",
    }
  ]
}
